[req]
default_bits = 2048
prompt = no
default_md = sha256
x509_extensions = v3_req
distinguished_name = dn

[dn]
C = US
ST = CA
L = Fremont
O = IT
OU = IT Department
emailAddress = webmaster@test.com
CN = *.test.com

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = *.test.com
DNS.3 = test.com
